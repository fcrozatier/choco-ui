{
  "name": "chocobytes",
  "version": "0.36.0",
  "type": "module",
  "license": "MIT",
  "description": "A Svelte UI-kit providing reusable building blocks to implement your own UI components and headless classes",
  "engines": {
    "node": "22.x",
    "pnpm": "9.x"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm build:docs && vite build && pnpm package",
    "build:docs": "node scripts/build-docs.js",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "pnpm package",
    "postpublish": "git push --follow-tags",
    "test": "pnpm test:integration && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "preprocessor",
    "!preprocessor/**/*.test.*",
    "scripts/bin.js",
    "scripts/utils.js",
    "src/lib/components/*"
  ],
  "exports": {
    "./preprocessor": "./preprocessor/index.js",
    "./components/*": "./dist/components/*",
    "./headless/*": "./dist/headless/*",
    "./blocks/*": "./dist/blocks/*",
    "./actions/*": "./dist/actions/*",
    "./utils/*": "./dist/utils/*",
    ".": "./dist/index.js"
  },
  "bin": {
    "chocobytes": "./scripts/bin.js"
  },
  "homepage": "https://choco-ui.com/",
  "keywords": [
    "svelte",
    "choco-ui",
    "chocobytes"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fcrozatier/choco-ui.git"
  },
  "peerDependencies": {
    "svelte": "^5.0.0-next.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@shikijs/rehype": "^1.18.0",
    "@sveltejs/adapter-node": "^5.2.4",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.5.28",
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "4.0.0-alpha.23",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/svelte": "^5.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest-axe": "^3.5.9",
    "@types/three": "^0.168.0",
    "happy-dom": "^14.12.3",
    "jest-axe": "^9.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "publint": "^0.2.11",
    "rehype-stringify": "^10.0.0",
    "rehype-target-plugin": "^1.0.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "remark-smartypants": "^3.0.2",
    "shiki": "^1.18.0",
    "svelte": "5.0.0-next.257",
    "svelte-check": "^4.0.2",
    "svelte-target-blank": "^1.0.0",
    "tailwindcss": "4.0.0-alpha.23",
    "three": "^0.168.0",
    "typescript": "^5.6.2",
    "unified": "^11.0.5",
    "vite": "^5.4.7",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@fcrozatier/ts-helpers": "^2.1.0",
    "clsx": "^2.1.1",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.11",
    "tailwind-merge": "^2.5.2"
  }
}
