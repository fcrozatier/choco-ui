{
  "name": "chocobytes",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "build:docs": "node scripts/build-docs.js",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "files": [
    "dist",
    "preprocessor",
    "!dist/**/*.test.*",
    "!preprocessor/**/*.test.*"
  ],
  "exports": {
    "./plugin": "./dist/plugin/index.js",
    "./preprocessor": "./preprocessor/index.js"
  },
  "imports": {
    "#plugin": "./dist/plugin/index.js"
  },
  "peerDependencies": {
    "svelte": "^5.0.0-next.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@shikijs/rehype": "^1.10.0",
    "@sveltejs/adapter-node": "^5.2.0",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/package": "^2.3.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/vite": "4.0.0-alpha.16",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/svelte": "^5.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest-axe": "^3.5.9",
    "happy-dom": "^14.12.3",
    "jest-axe": "^8.0.0",
    "mdsvex": "^0.11.2",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "publint": "^0.2.8",
    "rehype-stringify": "^10.0.0",
    "rehype-target-plugin": "^1.0.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "remark-smartypants": "^3.0.1",
    "shiki": "^1.10.1",
    "svelte": "5.0.0-next.160",
    "svelte-check": "^3.8.4",
    "svelte-target-blank": "^1.0.0",
    "tailwindcss": "4.0.0-alpha.16",
    "tslib": "^2.6.3",
    "typescript": "5.5.2",
    "unified": "^11.0.5",
    "vite": "^5.3.2",
    "vite-plugin-inspect": "^0.8.4",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@fcrozatier/ts-helpers": "^1.3.1",
    "clsx": "^2.1.1",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.10",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1"
  }
}
