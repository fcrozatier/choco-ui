{
  "name": "chocobytes",
  "version": "0.7.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm build:plugin && pnpm build:docs && vite build && pnpm package && pnpm build:dist",
    "build:dist": "node scripts/build-dist.js",
    "build:docs": "node scripts/build-docs.js",
    "build:plugin": "tsc --project ./plugin/tsconfig.json",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && pnpm build:dist && publint",
    "prepublishOnly": "pnpm package",
    "postpublish": "git push --follow-tags",
    "test": "pnpm test:integration && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "files": [
    "dist",
    "plugin",
    "preprocessor",
    "!dist/**/*.test.*",
    "!preprocessor/**/*.test.*",
    "!plugin/**/*.ts",
    "!plugin/**/*.test.*"
  ],
  "exports": {
    "./plugin": "./plugin/index.js",
    "./preprocessor": "./preprocessor/index.js",
    "./components/*": "./dist/components/*",
    "./headless/*": "./dist/headless/*",
    "./mixins/*": "./dist/mixins/*",
    "./actions/*": "./dist/actions/*"
  },
  "peerDependencies": {
    "svelte": "^5.0.0-next.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fcrozatier/choco-ui.git"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "@shikijs/rehype": "^1.12.1",
    "@sveltejs/adapter-static": "^3.0.3",
    "@sveltejs/kit": "^2.5.21",
    "@sveltejs/package": "^2.3.3",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/vite": "4.0.0-alpha.18",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/svelte": "^5.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest-axe": "^3.5.9",
    "happy-dom": "^14.12.3",
    "jest-axe": "^9.0.0",
    "mdsvex": "^0.11.2",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "publint": "^0.2.9",
    "rehype-stringify": "^10.0.0",
    "rehype-target-plugin": "^1.0.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "remark-smartypants": "^3.0.2",
    "shiki": "^1.12.1",
    "svelte": "5.0.0-next.217",
    "svelte-check": "^3.8.5",
    "svelte-target-blank": "^1.0.0",
    "tailwindcss": "4.0.0-alpha.18",
    "typescript": "^5.5.4",
    "unified": "^11.0.5",
    "vite": "^5.4.0",
    "vite-plugin-inspect": "^0.8.5",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@fcrozatier/ts-helpers": "^1.3.2",
    "clsx": "^2.1.1",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.11",
    "tailwind-merge": "^2.5.1"
  }
}
